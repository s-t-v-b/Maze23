<Window x:Class="Maze.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:Maze"
        mc:Ignorable="d"
        Title="Логин" Height="450" Width="800">
    <Grid>
        <PasswordBox x:Name="Pas_TB" HorizontalAlignment="Left" Margin="237,204,0,0" VerticalAlignment="Top" Width="100" Height="26"/>
        <Label x:Name="Pas_LB" Content="PASSWORD" HorizontalAlignment="Left" Margin="33,204,0,0" VerticalAlignment="Top"/>
        <Label x:Name="Log_LB" Content="LOGIN" HorizontalAlignment="Left" Margin="33,173,0,0" VerticalAlignment="Top"/>
        <TextBox x:Name="Log_TB" HorizontalAlignment="Left" Height="23" Margin="237,173,0,0" TextWrapping="Wrap" VerticalAlignment="Top" Width="120"/>
        <Button x:Name="Log_B" Content="ВХОД" HorizontalAlignment="Left" Margin="264,235,0,0" VerticalAlignment="Top" Width="75" Click="Log_B_Click"/>
        <Button x:Name="Reg_B" Content="Регистрация" HorizontalAlignment="Left" Margin="64,363,0,0" VerticalAlignment="Top" Width="75"/>
        <Button x:Name="Exit_B" Content="Выход" HorizontalAlignment="Left" Margin="282,368,0,0" VerticalAlignment="Top" Width="75"/>

    </Grid>
</Window>
